{
  "manifest_version": 2,
  "name": "Toolbar Dial",
  "version": "1.0.9",
  "permissions": ["bookmarks", "storage"],
  "description": "Toolbar Dial replaces the New Tab page. Bookmarks and folders in the \"Bookmarks bar\" folder will be shown in a grid like Opera.",
  "chrome_url_overrides": {
    "newtab": "dist/index.html"
  },
  "options_ui": {
    "page": "dist/options.html",
    "open_in_tab": true
  },
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png",
    "256": "icons/icon-256.png"
  }
}
